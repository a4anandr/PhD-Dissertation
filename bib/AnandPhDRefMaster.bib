@article{wil98,
	Abstract = {Let $Z(t), W(t), Y(t)$ be descriptive processes which measure congestion and delay in an open multiclass queueing network. Here $Z=(Z(t), t\geq0)$ is the so-called queue length process, $W=(W(t),\ t\geq0)$ is the workload process and $Y=(Y(t), t\geq0)$ is the cumulative idletime process. These processes are related to the processes $E, A$ and $D$, the so-called external, internal arrival and departure processes by model equations. The long-time behaviour of the network can be described by fluid scaling or diffusion scaling. In the fluid scaling of descriptive processes the time is accelerated by a factor $r^2$, say the rescaled external process is defined by $\overline E{}^r(t)=r^{-2}E^r(r^2t)$. In the diffusion scaling the rescaled process has the form $\hat E{}^r(t)=r^{-1}(E^r(r^2t)-\alpha^rr^2t)$. Substituting these processes into model equations leads to the fluid and diffusion scaled equations. The main results of the paper represent the functional central limit theorem for the diffusion scaled processes under the heavy traffic conditions: $(\hat W{}^r,\hat X{}^r,\hat Y{}^r,\hat Z{}^r)\to(W,X,Y,Z)$ as $r\to\infty$. Here the customers' numbers process $Z=\Delta W$ where $\Delta$ is the relevant linear operator and $(W,X,Z)$ satisfy the usual relations like $W=X+RY$ a.s. and $W$ is the semimartingale reflected Brownian motion (SRBM) associated with the data $(S,\theta,\Gamma,R,\nu)$. Here $\theta=R\gamma$ and $\Gamma=RHR'$ are the drift and covariance of the limiting diffusion and other parameters are defined in a conventional way.},
	Author = {Williams, R. J.},
	Date-Added = {2007-09-01 07:57:12 -0500},
	Date-Modified = {2007-09-01 07:57:12 -0500},
	Journal = QS,
	Number = {1-2},
	Pages = {27--88},
