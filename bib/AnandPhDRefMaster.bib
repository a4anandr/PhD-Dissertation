	Issn = {1064-8275},
	Journal = {SIAM J. Sci. Comput.},
	Mrclass = {65C05 (62F15 65C40 76M35 76S05)},
	Mrnumber = {MR2231730 (2007b:65009)},
	Mrreviewer = {Peter A. Lachenbruch},
	Number = {2},
	Pages = {776--803 (electronic)},
	Title = {Preconditioning {M}arkov chain {M}onte {C}arlo simulations using coarse-scale models},
	Volume = {28},
	Year = {2006},
	Bdsk-File-1 = {}}

